
public class BankAccountMain {

	public static void main(String[] args) {
		BankAccount buffaloSavings = new BankAccount("Buffalo");
		buffaloSavings.deposit(3);
		buffaloSavings.setOwner("Buffalo");

		BankAccount buffaloSecretOffshore = new BankAccount("Senor Buffalino");
		buffaloSecretOffshore.deposit (100000000);
		buffaloSecretOffshore.setOwner("Senor Buffalino");
		
		buffaloSavings.deposit(2.95);
		
		System.out.println(buffaloSavings.getBalance());
		System.out.println(buffaloSecretOffshore.getBalance());
		System.out.println(buffaloSavings.getOwner());
	}
	
}
